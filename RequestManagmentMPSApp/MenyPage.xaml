<Page x:Class="RequestManagmentMPSApp.MenyPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RequestManagmentMPSApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MenyPage" Loaded="Page_Loaded">

    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock HorizontalAlignment="Center" FontSize="16">Меню</TextBlock>
            <Button Margin="5,100,5,5" Width="160" Click="Button_Click">Пользователи</Button>
            <Button Margin="5" Width="160" Click="Button_Click_1">Заявки</Button>
            <Button Margin="5,10,5,5" Width="100" Click="Button_Click_2">Выход</Button>
        </StackPanel>
    </Grid>
</Page>
